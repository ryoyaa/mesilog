<div class="users-new-wrapper">
  <div class="container">
    <div class="card card-container">
      <img  class="profile-img-card" src="//ssl.gstatic.com/accounts/ui/avatar_2x.png" />
      <div class="row">
    <!--    <div class="col-md-offset-4 col-md-4 users-new-container">-->
          <h1 class="text-center text-black">Users Log in</h1>
          <%= form_for :session, url: login_path do |f| %>
            <div class="form-group ">
              <%= f.label :email, class: 'text-black' %>
              <%= f.text_field :email, class: 'form-control' %>
            </div>
            <div class="form-group ">
              <%= f.label :password, class: 'text-black' %>
              <%= f.password_field :password, class: 'form-control' %>
            </div>

            <%= f.submit "ログイン", class: 'btn square_btn' %>

          <% end %>
  <!--      </div>-->
      </div>
    </div>
  </div>
</div>
